node HelloKarma {
    fn decrement(n: int) -> int {
        return n - 1;
    }

    fn main() -> int {
        const limit: int = 100;
        var a: int = 0;
        var b: int = 1;
        var n: int = 10;

        while n > 0 && a < limit {
            c = a + b;
            a = b;
            b = c;

            n = decrement(n);
        }

        if a >= limit {
            return limit;
        } else {
            return a;
        }
    }
}